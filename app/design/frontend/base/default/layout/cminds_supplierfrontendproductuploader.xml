        <remove name="right"/>
        <remove name="left"/>
        <reference name="root">
            <action method="setTemplate"><template>page/1column.phtml</template></action>
        </reference>
        <reference name="head">

            <!-- <action method="removeItem"><type>skin_js</type><name>js/lib/jquery.cycle2.min.js</name></action>
